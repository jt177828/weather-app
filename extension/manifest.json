{
    "manifest_version": 3,
    "name": "Weather Extension",
    "version": "1.0",
    "description": "Simple weather extension using Express.js backend",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "geolocation"
    ],
    "host_permissions": [
      "http://localhost:3000/*"
    ]
  }